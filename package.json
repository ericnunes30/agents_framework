{
  "name": "agent-framework",
  "version": "1.0.0",
  "description": "Production-ready agent framework combining CrewAI's YAML simplicity with LangGraph's workflow power",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/server/index.js",
    "start:prod": "node dist/server/index.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:integration": "jest --config jest.config.integration.js",
    "test:unit": "jest --testPathPattern=tests --testPathIgnorePatterns=tests/integration",
    "test:http": "jest --testPathPattern=tests/http",
    "test:websocket": "jest --testPathPattern=tests/websocket",
    "test:middleware": "jest --testPathPattern=tests/http/middleware",
    "test:routes": "jest --testPathPattern=tests/http/routes",
    "test:services": "jest --testPathPattern=tests/http/services",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:verbose": "jest --verbose --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist/",
    "clean:all": "rm -rf dist/ node_modules/.cache",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "crewai",
    "langgraph",
    "mcp",
    "redis",
    "websocket"
  ],
  "author": "Agent Framework Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.45.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@sinonjs/fake-timers": "^14.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.11",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "babel-jest": "^30.0.5",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
